{
  "name": "PoliticoAI.WebApp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prebuild": "yarn prettify && yarn lint && tsc",
    "dev": "yarn prebuild && vite --base /PoliticoAI.WebApp/",
    "start": "yarn prebuild && vite  --base /PoliticoAI.WebApp/",
    "build": "yarn prebuild && vite build --base /Fugu/",
