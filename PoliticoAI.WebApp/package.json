    "buffer": "^6.0.3",
    "pdfjs-dist": "3.4.120",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons-kit": "^2.0.0",
    "react-modal": "^3.16.1",
    "react-native": "^0.74.2",
    "react-native-base64": "^0.2.1",
    "react-pdf": "^9.0.0"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.8",
