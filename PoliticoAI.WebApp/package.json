    "build:staging": "yarn prebuild && vite build --mode staging --outDir dist-staging --base /WebApp/",
    "lint": "eslint ./src --ext ts,tsx --fix",
    "prettify": "prettier ./src --write",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-pdf-viewer/core": "^3.12.0",
