    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "ncp": "^2.0.0",
    "prettier": "^3.2.5",
    "react-bootstrap": "^2.10.2",
    "react-redux": "^9.1.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.22.3",
